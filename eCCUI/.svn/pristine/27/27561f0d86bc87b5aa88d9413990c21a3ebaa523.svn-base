package com.hp.ecc.ui.biz.publish;

/**
 * This exception will be thrown out if publish fails.
 *
 * @author Jane Chen
 * @version 2.0
 */
public class PublishException extends Exception {
  public PublishException () {
    super ();
  }

  public PublishException ( String msg ) {
    super ( msg );
  }

  public PublishException ( Exception e ) {
    super ( "Error in publish process. Catch exception: " + e.toString () );
  }
}
